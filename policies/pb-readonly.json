{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3ReadOnlyOnPortfolioBucket",
      "Effect": "Allow",
      "Action": ["s3:GetObject","s3:ListBucket"],
      "Resource": [
        "arn:aws:s3:::dummy-nino-policy-mastery",
        "arn:aws:s3:::dummy-nino-policy-mastery/*"
      ]
    },
    {
      "Sid": "ReadOnlyDiscovery",
      "Effect": "Allow",
      "Action": ["ec2:Describe*","iam:Get*","iam:List*"],
      "Resource": "*"
    }
  ]
}
